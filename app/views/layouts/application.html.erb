<!DOCTYPE html>
<html>
<head>
  <title>MangaFeed</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:regular,bold,italic,thin,light,bolditalic,black,medium&amp;lang=en">
  <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
</head>

<body> 
  <div class="mdl-layout__container has-scrolling-header">
    <div class="app-layout mdl-layout mdl-layout--fixed-header mdl-js-layout mdl-color--grey-100">
      <%= render "shared/header" %>
      <div class="app-ribbon">
      </div>
      <main class="app-main mdl-layout__content">
        <div class="app-container mdl-grid">
          <%= yield %>
        </div>
        <%= render "shared/footer" %>
      </main>
    </div>
  </div>
</body>

</html>

